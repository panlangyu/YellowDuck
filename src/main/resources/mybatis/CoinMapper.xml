<mapper namespace="com.duck.yellowduck.domain.dao.CoinMapper">


    <!-- ������������������ -->
    <resultMap id="BaseResultMap" type="com.duck.yellowduck.domain.model.vo.CoinVo">
        <id column="id" jdbcType="INTEGER" property="id"/>
